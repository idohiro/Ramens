<h2>Results index</h2>
<table class="table table-hover table-inverse">
  <% if @range == "Customer" %>
    <tbody>
      <% @customers.each do |customer| %>
        <tr>
          <td><%= customer.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @ramens.each do |ramen| %>
        <tr>
          <td><%= ramen.name %></td>
          <td><%= ramen.introduction %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>